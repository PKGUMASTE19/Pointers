#include<stdio.h>
void swap1(int *a,int *b);
int main()
{
    int *a,*b;
    *a=10;
    *b=20;
       printf("value of a is %d\n", *a);
    printf("value of b is %d\n", *b);
    swap1(&a,&b);
    printf("value of a is %d\n", *a);
    printf("value of b is %d\n", *b);
}

void swap1(int *a,int *b)
{
    int temp;
    temp=*a;
    *a=*b;
    *b=temp;
        printf("value of a is %d\n", *a);
    printf("value of b is %d\n", *b);

}
